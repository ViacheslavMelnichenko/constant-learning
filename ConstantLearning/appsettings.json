{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Quartz": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=constantlearning;Username=postgres;Password=postgres"
  },
  "Telegram": {
    "BotToken": ""
  },
  "Schedule": {
    "RepetitionCron": "0 0 9 * * ?",
    "NewWordsCron": "0 0 20 * * ?"
  },
  "Learning": {
    "RepetitionWordsCount": 10,
    "NewWordsCount": 3,
    "AnswerDelaySeconds": 30
  },
  "Language": {
    "TargetLanguage": "Polish",
    "SourceLanguage": "Ukrainian",
    "TargetLanguageCode": "pl",
    "SourceLanguageCode": "uk"
  },
  "WordsImport": {
    "CsvPath": "/app/data/words.csv"
  }
}
